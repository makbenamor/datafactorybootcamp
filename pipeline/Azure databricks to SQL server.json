{
	"name": "Azure databricks to SQL server",
	"properties": {
		"activities": [
			{
				"name": "ForEach1",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Set variable1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@variables('the_names')",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "Copy from lakehouse to SQL",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "AzureDatabricksDeltaLakeSource",
									"exportSettings": {
										"type": "AzureDatabricksDeltaLakeExportCommand"
									}
								},
								"sink": {
									"type": "SqlServerSink",
									"writeBehavior": "insert",
									"sqlWriterUseTableLock": false
								},
								"enableStaging": true,
								"stagingSettings": {
									"linkedServiceName": {
										"referenceName": "AzureDataLakeStorage1",
										"type": "LinkedServiceReference"
									}
								},
								"translator": {
									"type": "TabularTranslator",
									"mappings": [
										{
											"sink": {
												"name": "ProductID",
												"type": "Int32"
											}
										},
										{
											"source": {
												"name": "name",
												"type": "String"
											},
											"sink": {
												"name": "Name",
												"type": "String"
											}
										},
										{
											"sink": {
												"name": "ProductNumber",
												"type": "String"
											}
										},
										{
											"sink": {
												"name": "Color",
												"type": "String"
											}
										},
										{
											"sink": {
												"name": "StandardCost",
												"type": "Decimal"
											}
										},
										{
											"sink": {
												"name": "ListPrice",
												"type": "Decimal"
											}
										},
										{
											"sink": {
												"name": "Size",
												"type": "String"
											}
										},
										{
											"sink": {
												"name": "Weight",
												"type": "Decimal"
											}
										},
										{
											"sink": {
												"name": "ProductCategoryID",
												"type": "Int32"
											}
										},
										{
											"sink": {
												"name": "ProductModelID",
												"type": "Int32"
											}
										},
										{
											"sink": {
												"name": "SellStartDate",
												"type": "DateTime"
											}
										},
										{
											"sink": {
												"name": "SellEndDate",
												"type": "DateTime"
											}
										},
										{
											"sink": {
												"name": "DiscontinuedDate",
												"type": "DateTime"
											}
										},
										{
											"sink": {
												"name": "ThumbNailPhoto",
												"type": "Byte[]"
											}
										},
										{
											"sink": {
												"name": "ThumbnailPhotoFileName",
												"type": "String"
											}
										},
										{
											"sink": {
												"name": "rowguid",
												"type": "Guid"
											}
										},
										{
											"sink": {
												"name": "ModifiedDate",
												"type": "DateTime"
											}
										}
									]
								}
							},
							"inputs": [
								{
									"referenceName": "AzureDatabricksDeltaLakeDataset1",
									"type": "DatasetReference",
									"parameters": {
										"table_name": {
											"value": "@item()",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "on_premdb",
									"type": "DatasetReference"
								}
							]
						}
					]
				}
			},
			{
				"name": "Set variable1",
				"type": "SetVariable",
				"dependsOn": [],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "circuits",
					"value": "circuits"
				}
			}
		],
		"variables": {
			"the_names": {
				"type": "Array",
				"defaultValue": [
					"circuits",
					"constructors",
					"drivers",
					"laptimes",
					"pitstops",
					"qualifying",
					"races",
					"results"
				]
			},
			"circuits": {
				"type": "String"
			}
		},
		"annotations": []
	}
}
{
	"name": "Azure databricks to SQL server",
	"properties": {
		"activities": [
			{
				"name": "ForEach1",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Get Metadata1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('Get Metadata1').output.structure.physicalName",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "Wait1",
							"type": "Wait",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"waitTimeInSeconds": 2
							}
						}
					]
				}
			},
			{
				"name": "Get Metadata1",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "on_premdb_races_static",
						"type": "DatasetReference"
					},
					"fieldList": [
						"structure"
					]
				}
			}
		],
		"variables": {
			"the_names": {
				"type": "Array",
				"defaultValue": [
					"circuits",
					"constructors",
					"drivers",
					"laptimes",
					"pitstops",
					"qualifying",
					"races",
					"results"
				]
			},
			"circuits": {
				"type": "String"
			}
		},
		"annotations": []
	}
}